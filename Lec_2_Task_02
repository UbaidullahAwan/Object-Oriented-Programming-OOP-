#include<iostream>
using namespace std;
class BankAccount{
    private:
   int accountNumber;
   string accountHolder;
   float balance;

    public:

    BankAccount(string ah, int an, float bal){

       accountNumber=an;
       accountHolder=ah;
       balance=bal;
    }

    void displayaccountdetails(){
        cout<<"Account Holder: "<<accountHolder<<endl;
        cout<<"Account Number: "<<accountNumber<<endl;
        cout<<"Balance: "<<balance<<endl;
    }
};
int main(){
    BankAccount b1("Ubaidullah Awan",320637373, 50000);
    b1.displayaccountdetails();
    return 0;
}